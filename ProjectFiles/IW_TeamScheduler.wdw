     code : |1+
      procedure CalculateTextColorAccordingToBackgroundColor(local nOriginalColor is int) : int
      
      InitialColor	is Color
      ReturnColor		is Color
      
      
      // Stores the initial color
      InitialColor	= nOriginalColor
      
      // Calls the syntax with the color variables
      ReturnColor		= CalculateTextColorAccordingToBackgroundColor(InitialColor)
      
      
      // Returns the reverse color found
      return ReturnColor.Color
     type : 458752
   -
     name : CalculateTextColorAccordingToBackgroundColor
     internal_properties : HgAAAB4AAAAKXSOjl3hcdRJzGaDlcAc6QkThBf22KfZRSSLWIvylqfGpzfCsNIQfDls5SNZJdF9cd9UXiZt5XfcJCNl0TKQH2vUtz5APyiIpbsIXM3F+lVaRy+iFEYuyfS1pqD4mylIcBVshdHY+vK58qXCbF0DFkbTg5YpG+KmcxjIQoVsEoRwa
     procedure_id : 1469092950284770422
     type_code : 14
     code : |1+
      // Summary: Returns the reverse color adapted to the requested color
      // Syntax:
      //CalculateTextColorAccordingToBackgroundColor ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      //	InitialColor (Color): Initial color
      procedure CalculateTextColorAccordingToBackgroundColor(local InitialColor is Color) : Color
      
      nLightness	is int
      rLuminance	is real
      ReturnColor	is Color
      
      
      // Calculate the luminance of the color passed as parameter
      rLuminance = 1 - (0.299 * InitialColor.Red + 0.587 * InitialColor.Green + 0.114 * InitialColor.Blue)/255
      
      // According to the luminance, the text color must have a low or high lightness
      if rLuminance < 0.5 then
      	// Low luminosity
      	nLightness = 30
      else
      	// High luminosity
      	nLightness = 30
      end
      
      // The returned color keeps the shade and the saturation
      ReturnColor = HSL(ColorHue(InitialColor), ColorSaturation(InitialColor), nLightness)
      
      
      // Returns the reverse color found
      return ReturnColor
     type : 458753
