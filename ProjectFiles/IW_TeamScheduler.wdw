         procedure MyWindow( EventOrAPT )
         //EventOrAPT: Appointment or Event to draw
         
         // => SourceToScreen
         STC_Details							= EventOrAPT.Content
         STC_APTTitle						= EventOrAPT.Title
         
         // Applies a color that allows reading the captions regardless of the background color
         STC_Details.Color					= CalculateTextColorAccordingToBackgroundColor(EventOrAPT.backgroundColor)
         STC_APTTitle.Color					= CalculateTextColorAccordingToBackgroundColor(EventOrAPT.backgroundColor)
         
         // Applies the color of the appointment directly on the internal window
         MyInternalWindow..BackgroundColor	= EventOrAPT.backgroundColor
         
         // If the internal window does not allow displaying the details, it hides the control
         if MyInternalWindow..Height <= MyInternalWindow..InitialHeight then
         	STC_Details.Visible = False
         end
         
